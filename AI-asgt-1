#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;

struct point{
	float x;
	float y;
};

void trail(ll n){
	for(ll i=0;i<n;i++){
		cout<<i<<" ";
	}
	cout<<endl;
}


int main(){
	
	ll n;
	string iseuclidean;
	cin>>iseuclidean>>n;
	vector<point> cities(n);
	for(ll i = 0;i<n;i++){
		cin>>cities[i].x>>cities[i].y;
	}
	vector<vector<float>> dist(n,vector<float>(n));
	for(ll i =0;i<n;i++){
		for(ll j=0;j<n;j++){
			cin>>dist[j][i];
		}
	}
	trail(n);
	return 0;
}

