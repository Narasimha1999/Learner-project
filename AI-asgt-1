#include<bits/stdc++.h>
using namespace std;

struct point{
	float x;
	float y;
};

void trail(int n){
	for(int i=0;i<n;i++){
		cout<<i<<" ";
	}
	cout<<endl;
}


int main(){
	
	int n;
	string iseuclidean;
	cin>>iseuclidean>>n;
	vector<point> cities(n);
	for(int i = 0;i<n;i++){
		cin>>cities[i].x>>cities[i].y;
	}
	vector<vector<float>> dist(n,vector<float>(n));
	for(int i =0;i<n;i++){
		for(int j=0;j<n;j++){
			cin>>dist[j][i];
		}
	}
	trail(n);
	return 0;
}

